@* ---------------Research Innovation Funding------------- *@
<div id="research-innovation-funding-type" style="display:none">
    <div class="form-group">
        <label class="font-weight-bold text-dark" for="asset-finance">@L("ResearchInnovationFundingOptions")</label>
        <select class="form-control" id="research-innovation-funding-type" name="researchinnovationfundingtype">
            <option disabled selected value="">@L("SelectAnOption")</option>
            <option value="5acb25ce62ba593724e0a786">Developing a new product/process</option>
            <option value="5acb25dd62ba593724e0a787">Filing a patent</option>
            <option value="59d26a5320070a604097b051">Funding for green initiatives</option>
            <option value="5acb25f862ba593724e0a788">New product commercialisation</option>
            <option value="5acb260462ba593724e0a789">Research funding</option>
            <option value="59d26a4820070a604097b050">Tech innovation</option>
