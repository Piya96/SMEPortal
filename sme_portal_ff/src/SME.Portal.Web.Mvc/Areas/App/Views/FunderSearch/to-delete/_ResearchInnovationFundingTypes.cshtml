</div>

@* New Product Commercialization  *@
<div id="research-innovation-new-product-commercialisation" style="display:none">

    <div class="form-group">
        <label for="" class="font-weight-bold text-dark" style="width:80%;">@L("AreYouCurrentlyAMemberOfAUniversity")</label>
        <div class="input-group mb-2 mb-sm-0 float-right" style="width: 110px;">
            <div class="radio-inline">
                <label class="radio radio-outline radio-primary">
                    <input type="radio" name="commresstudentstatus" value="Yes" />
                    <span></span>
                    @L("Yes")
                </label>
                <label class="radio radio-outline radio-primary">
                    <input type="radio" name="commresstudentstatus" value="No" />
